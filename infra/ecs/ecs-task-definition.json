{
  "family": "aws-containerized-app-task",
  "executionRoleArn": "arn:aws:iam::783141779211:role/awscontainerizedappecsTaskExecutionRole",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512",
  "containerDefinitions": [
    {
      "name": "aws-containerized-app",
      "image": "783141779211.dkr.ecr.eu-central-1.amazonaws.com/awscontainerizedapp:latest",
      "memory": 512,
      "cpu": 256,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "protocol": "tcp"
        }
      ]
    }
  ]
}
